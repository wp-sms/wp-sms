{
  "name": "wp-sms-settings-react-vite",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "watch": "WATCH=true vite build",
    "build": "vite build && pnpm run build:legacy",
