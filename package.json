{
  "name": "wp-sms",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "build": "wp-scripts build --webpack-src-dir=assets/src/blocks --output-path=assets/blocks && rm -rf assets/blocks/dev",
    "start": "concurrently \"wp-scripts start --webpack-copy-php --webpack-src-dir=assets/src/blocks --output-path=assets/blocks\" \"npm run dev\"",
    "sass-compile": "concurrently \"node-sass assets/src/scss/front-styles.scss assets/css/front-styles.css --output-style compressed\" \"node-sass assets/src/scss/mail.scss assets/css/mail.css --output-style compressed\" \"node-sass assets/src/admin/admin.scss assets/css/admin.css --output-style compressed\" \"node-sass assets/src/scss/onboarding/main.scss assets/css/onboarding.min.css --output-style compressed\"",
    "watch-sass": "concurrently \"node-sass assets/src/scss/front-styles.scss assets/css/front-styles.css --watch --output-style compressed\" \"node-sass assets/src/scss/mail.scss assets/css/mail.css --watch --output-style compressed\" \"node-sass assets/src/admin/admin.scss assets/css/admin.css --watch --output-style compressed\" \"node-sass assets/src/scss/onboarding/main.scss assets/css/onboarding.min.css --watch --output-style compressed\"",
    "js-compile": "webpack --config webpack.scripts.config.js",
    "watch-js": "webpack --config webpack.scripts.config.js --watch",
    "watch": "concurrently \"npm run watch-sass\" \"npm run watch-js\"",
    "dev": "npm run watch",
    "settings:dev": "vite --config vite.config.js",
    "settings:build": "vite build --config vite.config.js",
    "settings:preview": "vite preview --config vite.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "wp:docker:start": "wp-env start",
    "wp:docker:stop": "wp-env stop",
    "wp:docker:clean": "wp-env clean all",
    "wp:docker:logs": "wp-env logs",
    "wp:docker:cli": "wp-env run cli",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:smoke": "playwright test --grep @smoke",
    "e2e:subscribers": "playwright test e2e/tests/subscribers/",
    "e2e:groups": "playwright test e2e/tests/groups/",
    "e2e:send-sms": "playwright test e2e/tests/send-sms/",
    "e2e:outbox": "playwright test e2e/tests/outbox/",
    "e2e:report": "playwright show-report e2e-report",
    "e2e:codegen": "playwright codegen http://localhost:8888"
  },
  "keywords": [],
  "author": "",
  "license": "GPLv3",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/runtime-corejs2": "^7.0.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.3.0",
    "@wordpress/babel-preset-default": "^2.1.0",
    "@wordpress/components": "^23.8.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^5.6.0",
    "@wordpress/env": "^10.28.0",
    "@wordpress/scripts": "^30.17.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^10.0.0",
    "breakpoint-sass": "^2.7.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.5.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^7.1.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.94.1",
    "shadcn": "^3.6.2",
    "style-loader": "^4.0.0",
    "svg-inline-loader": "^0.8.2",
    "tailwindcss": "^3.4.0",
    "terser": "^5.44.1",
    "terser-webpack-plugin": "^5.3.10",
    "vite": "^5.4.0",
    "vite-plugin-compression": "^0.5.1",
    "watch": "^0.13.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@woocommerce/dependency-extraction-webpack-plugin": "^3.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "fast-deep-equal": "^3.1.3",
    "lucide-react": "^0.400.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tailwind-merge": "^2.2.0"
  }
}
