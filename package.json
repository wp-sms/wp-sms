{
  "name": "wp-sms",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "npm run build:blocks && npm run build:legacy && npm run build:react && npm run i18n:extract",
    "build:blocks": "wp-scripts build && rm -rf public/blocks/dev",
    "build:legacy": "vite build --config vite.config.legacy.js",
    "build:react": "vite build --config vite.config.react.ts",
    "i18n:extract": "npm run i18n:build && npm run i18n:extract:all && npm run i18n:fix-paths && npm run i18n:cleanup",
    "i18n:build": "vite build --config vite.config.i18n.ts",
    "i18n:extract:all": "wp i18n make-pot . languages/wp-sms.pot --exclude=node_modules,vendor,resources --include=includes,src,public/i18n-temp,public/blocks,public/legacy",
    "i18n:fix-paths": "node resources/fix-pot-paths.js",
    "i18n:cleanup": "rm -rf public/i18n-temp",
    "start": "npm run dev",
    "dev": "npm run dev:react",
    "dev:legacy": "vite build --config vite.config.legacy.js --watch --mode development",
    "dev:react": "vite --config vite.config.react.ts",
    "watch": "npm run dev:legacy",
    "watch:legacy": "vite build --config vite.config.legacy.js --watch",
    "watch:react": "vite build --config vite.config.react.ts --watch",
    "preview:react": "vite preview --config vite.config.react.ts"
  },
